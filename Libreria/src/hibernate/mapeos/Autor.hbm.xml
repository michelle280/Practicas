<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
<class name="hibernate.beans.Autor" table="Autor">
    <id name="idAutor" column="idAutor" type="int"/>
     <property name="Nombre" column="Nombre" type="string"/>
     <property name="Apellidopaterno" column="Apellido_Paterno" type="string"/>
     <property name="Apellidomaterno" column="Apellido_Materno" type="string"/>
     <property name="Fechanacimiento" column="Fecha_Nacimiento" type="date"/>
     <property name="Pais" column="Pais" type="string"/>
</class>
</hibernate-mapping>
    
    

